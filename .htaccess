
<IfModule mod_rewrite.c>

############################################
## enable rewrites

    Options +FollowSymLinks
    RewriteEngine on

############################################
## you can put here your magento root folder
## path relative to web root

    #RewriteBase /magento/


############################################
## rewrite everything else to index.php

    RewriteRule .* index.php [L]

</IfModule>


